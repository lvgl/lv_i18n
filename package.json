    "cldr-core": "^41.0.0",
    "debug": "^4.3.4",
    "es6-error": "^4.1.1",
    "glob": "^8.0.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "shelljs": "^0.8.5"
  },
  "bundledDependencies": [
    "argparse",
    "cldr-core",
    "debug",
    "es6-error",
    "glob",
    "js-yaml",
    "lodash",
    "make-plural",
    "shelljs"
  ],
  "devDependencies": {
    "eslint": "^8.15.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "shx": "^0.3.4"
