{
  "name": "lv_i18n",
  "version": "0.2.1",
  "description": "CLI tools for LittlevGL i18n support.",
  "keywords": [
    "i18n",
    "internationalization"
  ],
  "repository": "littlevgl/lv_i18n",
  "license": "MIT",
  "files": [
    "lv_i18n.js",
    "lib/",
    "src/"
  ],
  "bin": {
    "lv_i18n": "lv_i18n.js"
  },
  "scripts": {
    "lint": "eslint .",
